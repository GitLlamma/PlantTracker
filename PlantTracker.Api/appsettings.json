{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Information",
      "Microsoft.EntityFrameworkCore.Database.Command": "Warning",
      "Microsoft.EntityFrameworkCore.Migrations": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=planttracker.db"
  },
  "JwtSettings": {
    "Secret": "REPLACE_WITH_SECRET_IN_AZURE_APP_SETTINGS",
    "Issuer": "PlantTracker.Api",
    "Audience": "PlantTracker.App",
